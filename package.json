{
  "name": "dora-engine",
  "version": "0.0.4",
  "description": "",
  "main": "index.js",
  "dependencies": {
    "@google-cloud/speech": "^4.5.0",
    "@google-cloud/text-to-speech": "^3.2.0",
    "@google-cloud/translate": "^6.2.0",
    "@types/dotenv": "^8.2.0",
    "aws-iot-device-sdk": "^2.1.0",
    "aws-sdk": "^2.418.0",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "color-rgba": "^2.1.0",
    "cookie-parser": "^1.4.3",
    "core-io-types": "^2.1.0",
    "csurf": "^1.9.0",
    "date-utils": "^1.2.21",
    "dayjs": "^1.11.9",
    "dora": "github:yamagame/dora",
    "dotenv": "^16.3.1",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "googleapis": "^73.0.0",
    "ip": "^1.1.5",
    "jsdom": "^20.0.3",
    "jws": "^3.1.5",
    "mecab-async": "^0.1.2",
    "memorystore": "^1.6.0",
    "mkdirp": "^0.5.1",
    "multer": "^1.4.5-lts.1",
    "mustache": "^2.3.0",
    "node-fetch": "^2.1.2",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "ping": "^0.2.2",
    "ps-tree": "^1.1.0",
    "pug": "^3.0.2",
    "sequelize": "^5.16.0",
    "socket.io": "^4.4.1",
    "socket.io-client": "^4.4.1",
    "sqlite3": "^4.0.0",
    "wav": "^1.0.2"
  },
  "optionalDependencies": {
    "pigpio": "^3.3.1",
    "raspi": "^6.0.1",
    "raspi-gpio": "^6.2.2",
    "raspi-soft-pwm": "^6.0.2",
    "ulid": "^2.3.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "jest": "^29.6.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.7",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  },
  "scripts": {
    "start": "SPEECH=off ts-node -r tsconfig-paths/register src/robot-server.ts",
    "start:mac": "SPEECH=off ROBOT_VOICE=mac ts-node -r tsconfig-paths/register src/robot-server.ts",
    "start:browser": "SPEECH=browser ts-node -r tsconfig-paths/register src/robot-server.ts",
    "whisper": "SPEECH=whisper WHISPER_PATH=./whisper.sh ts-node -r tsconfig-paths/register src/robot-server.ts",
    "whisper:mac": "SPEECH=whisper WHISPER_PATH=./whisper.sh ROBOT_VOICE=mac ts-node -r tsconfig-paths/register src/robot-server.ts",
    "reazon": "REAZON_SPEECH_HOST=host.docker.internal REAZON_SPEECH_PORT=3093 SPEECH=reazon ts-node -r tsconfig-paths/register src/robot-server.ts",
    "reazon:mac": "REAZON_SPEECH_HOST=localhost REAZON_SPEECH_PORT=3093 SPEECH=reazon ROBOT_VOICE=mac ts-node -r tsconfig-paths/register src/robot-server.ts",
    "robot": "node build/src/robot-server.js",
    "robot:db": "SPEECH=off ROBOT_VOICE=mac ROBOT_DB=true node build/src/robot-server.js",
    "robot:mac": "SPEECH=off ROBOT_VOICE=mac node build/src/robot-server.js",
    "servo": "node build/src/servo-head.js",
    "servo:sudo": "sudo node build/src/servo-head.js",
    "build": "rm -rf ./build && tsc --project tsconfig.build.json && tsc-alias --verbose -p tsconfig.build.json",
    "ts-node": "ts-node -r tsconfig-paths/register $1",
    "test": "jest"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "transform": {
      "^.+\\.ts$": [
        "ts-jest",
        {
          "tsconfig": "tsconfig.json"
        }
      ]
    },
    "testMatch": [
      "**/*.test.ts"
    ],
    "moduleNameMapper": {
      "^~/(.+)": "<rootDir>/src/$1"
    },
    "preset": "ts-jest"
  },
  "keywords": [],
  "author": "",
  "license": "MIT"
}
